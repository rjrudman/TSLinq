{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "./ts-out",
    "sourceMap": false,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "downlevelIteration": true,
    "strict": true,
    "target": "es5",
    "lib": [
      "es2015",
      "dom"
    ]
  }
}